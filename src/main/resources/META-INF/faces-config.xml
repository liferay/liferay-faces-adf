<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns:bridge="http://www.liferay.com/xml/ns/liferay-faces-bridge-2.0-extension"
			  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<name>LiferayFacesADF</name>
	<ordering>
		<after>
			<name>adfv</name>
		</after>
	</ordering>
	<factory>
		<external-context-factory>com.liferay.faces.adf.internal.context.ExternalContextFactoryLiferayADFImpl</external-context-factory>
		<faces-context-factory>com.liferay.faces.adf.internal.context.FacesContextFactoryLiferayADFImpl</faces-context-factory>
		<render-kit-factory>com.liferay.faces.adf.internal.render.RenderKitFactoryLiferayADFImpl</render-kit-factory>
		<factory-extension>
			<bridge:bridge-portlet-request-factory>com.liferay.faces.adf.internal.filter.BridgePortletRequestFactoryADFImpl</bridge:bridge-portlet-request-factory>
		</factory-extension>
	</factory>
</faces-config>
